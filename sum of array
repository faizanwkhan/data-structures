#include <stdio.h>
 #include <conio.h>
int main()
{
    int a[1000],i,n,sum=0;
    printf("Enter size of the array : ");
    scanf("%d",&n);
    printf("Enter elements in array : ");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0; i<n; i++)
    {
         
        sum+=a[i];
    }
     printf("sum of array is : %d",sum);
    return 0;
}
output:Enter size of the array : 4
Enter elements in array : 3
5
7
9
sum of array is : 24

...Program finished with exit code 0
