#include<stdio.h>
int main()
{
	int age;
	printf("Enter your age:");
	scanf("%d",&age);
	if(age>=18)
	{
	   printf("person is eligble to vote");
      }
      else
      {
         printf("person is not eligble to vote");
      }
      return 0;
}
output:Enter your age:18
person is eligble to vote

...Program finished with exit code 0
